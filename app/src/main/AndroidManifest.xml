<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tienda.app.splash.aplicacionsplash">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="tienda.app.splash.aplicacionsplash.SignUp"></activity>
        <activity android:name="tienda.app.splash.aplicacionsplash.Login" />
        <activity android:name="tienda.app.splash.aplicacionsplash.splash_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="tienda.app.splash.aplicacionsplash.MainActivity" />
    </application>

</manifest>